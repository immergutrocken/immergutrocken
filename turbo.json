{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "outputs": ["dist/**", "build/**"]
    },
    "build": {
      "outputs": ["public/**", ".next/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "outputs": ["dist/**", "build/**"]
    },
    "test:ci": {
      "outputs": ["dist/**", "build/**"]
    }
  }
}
