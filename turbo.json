{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "dev": {
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "build": {
      "outputs": [
        "public/**",
        ".next/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "outputs": [
        "dist/**",
        "build/**"
      ]
    }
  }
}
