{
  "name": "immergutrocken",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:ci": "turbo run test:ci",
    "lint": "turbo run lint",
    "vercel:pull-env-website": "vercel env pull --environment=development --yes --cwd apps/website",
    "vercel:pull-env-cms": "vercel env pull --environment=development --yes --cwd apps/cms",
    "vercel:login": "vercel login",
    "vercel:link-website": "vercel link --cwd apps/website",
    "vercel:link-cms": "vercel link --cwd apps/cms",
    "prepare": "husky"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "turbo": "2.4.0"
  },
  "devDependencies": {
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "vercel": "40.1.0"
  },
  "overrides": {
    "@portabletext/editor": "1.28.0",
    "eslint": "8.57.1"
  },
  "packageManager": "npm@10.8.2"
}
