{
  "name": "immergutrocken",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:ci": "turbo run test:ci",
    "lint": "turbo run lint",
    "vercel:pull-env-website": "vercel env pull --environment=development --yes --cwd apps/website",
